{
  "all": true,
  "include": [
    "apps/**/*.tsx",
    "apps/**/*.ts",
    "packages/**/*.tsx",
    "packages/**/*.ts"
  ],
  "exclude": [
    "**/*.styles.ts",
    "**/*.test.ts",
    "**/*.mock.ts",
    "**/*.test.tsx",
    "**/*.styles.tsx",
    "**/*.model.ts",
    "**/*.interface.ts",
    "**/*.enum.ts",
    "**/*.d.ts",
    "**/index.ts",
    "**/*.url.ts"
  ],
  "check-coverage": false,
  "branches": 70,
  "lines": 70,
  "functions": 70,
  "statements": 70
}
